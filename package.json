{
  "name": "a-frame_learn",
  "version": "1.0.0",
  "description": "my A-Frame Scene!For Psychology",
  "scripts": {
    "fastapi:dev": "uvicorn backend.main:app --reload --host 0.0.0.0 --port 8000",
    "fastapi:prod": "uvicorn backend.main:app --host 0.0.0.0 --port 8000",
    "aframe:dev": "cross-env NODE_ENV=development snowpack dev --no-hmr",
    "build": "cross-env NODE_ENV=production snowpack build",
    "serve": "serve -s build -p 80",
    "start:dev": "pnpm run aframe:dev && pnpm run fastapi:dev",
    "start:prod": "NODE_ENV=production pnpm run aframe:build && pnpm run serve:prod && pnpm run fastapi:dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Schweik7/A-Frame_learn.git"
  },
  "keywords": [
    "aframe",
    "js",
    "ts"
  ],
  "author": "Schweik7",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Schweik7/A-Frame_learn/issues"
  },
  "homepage": "https://github.com/Schweik7/A-Frame_learn#readme",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "cross-env": "^7.0.3",
    "snowpack": "^3.8.8",
    "webpack": "^5.91.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.0.4"
  },
  "dependencies": {
    "@parcel/runtime-browser-hmr": "^2.12.0",
    "dotenv-flow": "^4.1.0",
    "serve": "^14.2.3"
  }
}
